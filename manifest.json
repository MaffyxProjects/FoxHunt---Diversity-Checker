{
    "name": "FoxHunt - Diversity Checker",
    "manifest_version": 3,
    "version": "1.0.00",
    "description": "AutoSourcing For Diversity Candidates on LinkedIn Recruiter",
    "author": "Matt Fox",
    "icons": {
              "128": "/images/icons/icon128.png" },

    "action": {
        "default_popup": "popup.html"
    },
    "background": {
        "service_worker": "background.js"
      },

    "permissions": [
        "tabs","storage","activeTab","scripting"
    ]
}
